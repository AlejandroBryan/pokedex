{
  "name": "simple-js-app",
  "version": "1.0.0",
  "description": "pokedex",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "sass": "node-sass src/scss/style.scss --output-style compact -o src/css",
    "babel": "babel ./src --out-dir dist",
    "dev": "npm run clean && npm run babel && npm run sass && webpack-dev-server --config config/webpack.dev.js --open",
    "build": "npm run clean && npm run babel  && npm run sass && webpack --config config/webpack.prod.js",
    "lint": "eslint -c .eslintrc --ext .js src",
    "fix": "eslint --fix --ext .js src ",
    "format": "prettier --config .prettierrc --write .js src/js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlejandroBryan/simple-js-app.git"
  },
  "keywords": [
    "js",
    "fetch",
    "ajax",
    "functional",
    "programming",
    "css",
    "html",
    "DOM"
  ],
  "author": "Alejandro Bryan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlejandroBryan/simple-js-app/issues"
  },
  "homepage": "https://github.com/AlejandroBryan/simple-js-app#readme",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-exclude-assets-plugin": "^0.0.7",
    "html-webpack-plugin": "^5.5.0",
    "ignore-emit-webpack-plugin": "^2.0.6",
    "mini-css-extract-plugin": "^2.7.0",
    "node-sass": "^8.0.0",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "purgecss-webpack-plugin": "^5.0.0",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpack-remove-empty-scripts": "^1.0.1"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.2",
    "jquery": "^3.6.1",
    "popper.js": "^1.16.1",
    "promise-polyfill": "8.2.3",
    "whatwg-fetch": "^3.6.2"
  }
}
